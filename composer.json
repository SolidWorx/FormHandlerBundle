{
    "name": "solidworx/form-handler-bundle",
    "description": "Easy form handling for Symfony forms",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords" : [
        "symfony",
        "symfony-bundle",
        "symfony forms",
        "form handling"
    ],
    "authors": [
        {
            "name": "Pierre du Plessis",
            "email": "open-source@solidworx.co.za"
        }
    ],
    "autoload": {
        "psr-4": {
            "SolidWorx\\FormHandler\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolidWorx\\FormHandler\\Tests\\": "./tests"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "symfony/framework-bundle": "^4.0 || ^5.0",
        "symfony/form": "^4.0 || ^5.0",
        "lodash-php/lodash-php": "^0.0.5"
    },
    "require-dev": {
        "doctrine/common": "^2.7",
        "ocramius/proxy-manager": "^2.1",
        "symfony/phpunit-bridge": "^5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
