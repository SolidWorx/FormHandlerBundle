{
    "name": "solidworx/form-handler-bundle",
    "description": "Easy form handling for Symfony forms",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords" : [
        "symfony",
        "symfony-bundle",
        "symfony forms",
        "form handling"
    ],
    "authors": [
        {
            "name": "Pierre du Plessis",
            "email": "pdples@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SolidWorx\\FormHandler\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolidWorx\\FormHandler\\Tests\\": "./tests"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "symfony/framework-bundle": "^3.0",
        "symfony/form": "^2.7 || ^3.0",
        "solidworx/util": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "doctrine/common": "^2.7",
        "ocramius/proxy-manager": "^2.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
